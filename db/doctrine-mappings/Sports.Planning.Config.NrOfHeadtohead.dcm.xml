<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sports\Planning\Config\NrOfHeadtohead" table="nrOfHeadtoheads" repository-class="Sports\Planning\Config\NrOfHeadtohead\Repository">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="amount" column="amount" type="smallint" nullable="false" unique="false"/>
    <many-to-one field="competitionSport" target-entity="Sports\Competition\Sport">
      <join-columns>
        <join-column name="competitionSportId" referenced-column-name="id" nullable="true" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="planningConfig" target-entity="Sports\Planning\Config" inversed-by="nrOfheadtoheads">
      <join-columns>
        <join-column name="planningConfigId" referenced-column-name="id" nullable="true" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>