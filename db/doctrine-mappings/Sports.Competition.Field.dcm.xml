<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Sports\Field" table="fields" repository-class="Sports\Field\Repository">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="priority" column="priority" type="smallint" nullable="false" unique="false"/>
        <field name="name" column="name" type="string" length="3" nullable="true" unique="false"/>
        <many-to-one field="competition" target-entity="Sports\Competition" inversed-by="fields">
            <join-columns>
                <join-column name="competitionId" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="competitionSport" target-entity="Sports\Competition\Sport">
            <join-columns>
                <join-column name="competitionSport" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="sportConfigDep" target-entity="Sports\Sport\Config">
            <join-columns>
                <join-column name="sportConfigIdDep" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
            </join-columns>
        </many-to-one>
    </entity>
</doctrine-mapping>